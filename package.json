{
  "name": "llm-marketplace",
  "version": "1.1.0",
  "description": "Enterprise LLM Marketplace Platform - Discovery, Publishing, and Consumption of LLM Services",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/globalbusinessadvisors/llm-marketplace.git"
  },
  "author": "LLM Marketplace Team",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "workspaces": [
    "services/publishing",
    "services/admin",
    "services/model-marketplace",
    "services/tenant-management",
    "services/graphql-gateway",
    "sdks/javascript",
    "tests/e2e"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build:publishing": "npm run build --workspace=services/publishing",
    "build:admin": "npm run build --workspace=services/admin",
    "build:model-marketplace": "npm run build --workspace=services/model-marketplace",
    "build:tenant-management": "npm run build --workspace=services/tenant-management",
    "build:graphql-gateway": "npm run build --workspace=services/graphql-gateway",
    "build:sdk": "npm run build --workspace=sdks/javascript",
    "dev": "npm run dev --workspaces --if-present",
    "dev:publishing": "npm run dev --workspace=services/publishing",
    "dev:admin": "npm run dev --workspace=services/admin",
    "test": "jest --coverage",
    "test:unit": "jest --testPathPattern='.*\\.test\\.ts$'",
    "test:integration": "jest --testPathPattern='.*\\.integration\\.ts$'",
    "test:e2e": "jest --testPathPattern='.*\\.e2e\\.ts$'",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist coverage node_modules/.cache",
    "prepare": "husky install",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "migrate:up": "npm run migrate:up --workspace=services/publishing",
    "migrate:down": "npm run migrate:down --workspace=services/publishing",
    "migrate:create": "npm run migrate:create --workspace=services/publishing",
    "seed": "npm run seed --workspace=services/publishing",
    "version:patch": "npm version patch --workspaces --no-git-tag-version",
    "version:minor": "npm version minor --workspaces --no-git-tag-version",
    "version:major": "npm version major --workspaces --no-git-tag-version",
    "prepublishOnly": "npm run build && npm run test",
    "publish:packages": "npm publish --workspaces --access public",
    "publish:dry-run": "npm publish --workspaces --access public --dry-run"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.14.0",
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/exporter-jaeger": "^1.19.0",
    "@opentelemetry/instrumentation": "^0.46.0",
    "@opentelemetry/sdk-trace-node": "^1.19.0",
    "@prisma/client": "^5.9.1",
    "axios": "^1.6.5",
    "claude-flow": "^2.7.31",
    "dotenv": "^16.4.1",
    "elasticsearch": "^16.7.3",
    "fastify": "^4.25.2",
    "ioredis": "^5.3.2",
    "joi": "^17.12.0",
    "kafkajs": "^2.2.4",
    "openai": "^4.24.7",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.11.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-security": "^2.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "prisma": "^5.9.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/services",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "services/**/*.ts",
      "!services/**/*.d.ts",
      "!services/**/index.ts",
      "!services/**/__tests__/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/services/$1"
    }
  }
}
